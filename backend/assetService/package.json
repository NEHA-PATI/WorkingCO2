  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.13.2",
    "backend": "file:..",
    "cloudinary": "^2.8.0",
    "co2-plus": "file:../..",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "joi": "^17.12.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.3",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.4",
    "supertest": "^6.3.4"
  },
  "nodemonConfig": {
    "quiet": true,
    "ignore": [
      "node_modules"
    ],
    "watch": [
      "server.js",
      "src",
      "routes",
      "controllers"
    ]
